#include <iostream>
#include <string>
using namespace std;

// ESTRUCTURA DEL NODO 

struct NodoGenealogico {
    string nombre;
    int generacion;  
    NodoGenealogico* padre;   
    NodoGenealogico* madre;   

    NodoGenealogico(string n, int g) : nombre(n), generacion(g), padre(nullptr), madre(nullptr) {}
};
